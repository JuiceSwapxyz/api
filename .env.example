# JuiceSwap Routing API Environment Configuration

# Server Configuration
PORT=3000
CORS_ORIGIN=http://localhost:3001

# RPC Endpoints - Alchemy (core EVM networks)
# Replace with your Alchemy API keys
ALCHEMY_1=your-alchemy-api-key          # Ethereum Mainnet
ALCHEMY_10=your-alchemy-api-key         # Optimism
ALCHEMY_137=your-alchemy-api-key        # Polygon
ALCHEMY_8453=your-alchemy-api-key       # Base
ALCHEMY_42161=your-alchemy-api-key      # Arbitrum
ALCHEMY_11155111=your-alchemy-api-key   # Sepolia (testnet)
ALCHEMY_5115=none

# RPC Provider Health Monitoring
RPC_PROVIDER_HEALTH_TABLE_NAME=rpc-provider-health-local

# Tenderly Configuration (optional - for gas simulation)
TENDERLY_USER=your_tenderly_username
TENDERLY_PROJECT=your_tenderly_project
TENDERLY_ACCESS_KEY=your_tenderly_access_key
TENDERLY_NODE_API_KEY=your_tenderly_node_api_key

# Uniswap GraphQL Configuration
UNI_GRAPHQL_ENDPOINT=https://api.uniswap.org/v1/graphql
UNI_GRAPHQL_HEADER_ORIGIN=https://app.uniswap.org

# Internal Configuration
UNICORN_SECRET=your_internal_api_secret