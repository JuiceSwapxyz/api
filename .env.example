# JuiceSwap Routing API Environment Configuration

# Server Configuration
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,https://app.juiceswap.com,https://dev.app.juiceswap.com

# Alchemy API Keys (RPC Providers)
# Replace with your Alchemy API keys
ALCHEMY_1=your-alchemy-api-key          # Ethereum Mainnet
ALCHEMY_10=your-alchemy-api-key         # Optimism
ALCHEMY_137=your-alchemy-api-key        # Polygon
ALCHEMY_8453=your-alchemy-api-key       # Base
ALCHEMY_42161=your-alchemy-api-key      # Arbitrum
ALCHEMY_11155111=your-alchemy-api-key   # Sepolia (testnet)
ALCHEMY_5115=none                       # Citrea (uses public RPC)

# Ponder API for Citrea Token List
PONDER_URL=https://ponder.juiceswap.com

# Citrea RPC URL
CITREA_RPC_URL=http://vm-dfx-node-dev.westeurope.cloudapp.azure.com:8085

# Rate Limiting
RATE_LIMIT_QUOTE_PER_MINUTE=2000
RATE_LIMIT_GENERAL_PER_MINUTE=10000

# Optional: RPC Call Logging (for debugging RPC usage - enabled by default)
# ENABLE_RPC_LOGGING=false

# PostgreSQL Database (required when OG_CAMPAIGN_ENABLED=true)
# Format: postgresql://username:password@host:port/database
DATABASE_URL="postgresql://user:password@localhost:5432/juiceswap"

# Enable OG Campaign tracking (requires DATABASE_URL to be set)
OG_CAMPAIGN_ENABLED=false

# OG Campaign OAuth Configuration (not yet implemented)
TWITTER_CLIENT_ID=your_twitter_client_id
TWITTER_CLIENT_SECRET=your_twitter_client_secret
TWITTER_CALLBACK_URL=https://app.juiceswap.com/twitter-callback
JUICESWAP_TWITTER_USER_ID=your_juiceswap_twitter_user_id
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret
DISCORD_CALLBACK_URL=https://app.juiceswap.com/discord-callback
DISCORD_GUILD_ID=your_discord_server_id

# First Squeezer Campaign OAuth Configuration
FIRST_SQUEEZER_TWITTER_CLIENT_ID=your_twitter_client_id
FIRST_SQUEEZER_TWITTER_CLIENT_SECRET=your_twitter_client_secret
FIRST_SQUEEZER_TWITTER_CALLBACK_URL=http://localhost:3001/first-squeezer/twitter/callback
FIRST_SQUEEZER_JUICESWAP_TWITTER_USER_ID=1975843807011700736
